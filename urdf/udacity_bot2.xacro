<?xml version='1.0'?>

<robot name="udacity_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find slam_project)/urdf/udacity_bot2.gazebo" />
  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <joint name="camera_joint" type="fixed">
    <origin xyz="0.15 0 0" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="camera" />
  </joint>

  <joint name="camera_joint2" type="fixed">
        <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
        <parent link="camera"/>
        <child link="camera_rgbd_frame"/>
  </joint>

  <joint name="hokuyo_joint" type="fixed">
    <origin xyz="0.075 0 0.05" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="hokuyo" />
  </joint>

  <joint name="hokuyo_joint2" type="fixed">
    <origin xyz="-0.075 0 0.05" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="hokuyo2" />
  </joint>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 0.075 0" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0 -0.075 0" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="R_wheel_hinge">
    <origin xyz="-0.125 -0.075 0" rpy="0 0 0"/>
    <child link="right_wheel2"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <joint type="continuous" name="L_wheel_hinge">
    <origin xyz="-0.125 0.075 0" rpy="0 0 0"/>
    <child link="left_wheel2"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="0" friction="0"/>
  </joint>






  <joint type="continuous" name="R3_wheel_hinge">
    <origin xyz="0.125 -0.075 0" rpy="0 0 0"/>
    <child link="right_wheel3"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="0" friction="0"/>
  </joint>

  <joint type="continuous" name="L3_wheel_hinge">
    <origin xyz="0.125 0.075 0" rpy="0 0 0"/>
    <child link="left_wheel3"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="0" friction="0"/>
  </joint>






  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".3 .1 .05"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".3 .1 .05"/>
      </geometry>
      <material name="blue">
        <color rgba="${0/255} ${0/255} ${255/255} 1.0"/>
      </material>
    </visual>


    <!--collision name='back_caster_collision'>
      <origin xyz="-0.075 0 -0.025" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.02499"/>
      </geometry>
    </collision>
    <visual name='back_caster_visual'>
      <origin xyz="-0.075 0 -0.025" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
    </visual>
    <collision name='front_caster_collision'>
      <origin xyz="0.075 0 -0.025" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.02499"/>
      </geometry>
    </collision>
    <visual name='front_caster_visual'>
      <origin xyz="0.075 0 -0.025" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
    </visual-->

  </link>

  <link name='left_wheel'>
    <pose>0 0 0 0 0 0</pose>

    <inertial>
      <mass value="5"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>
    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.025"/>
      </geometry>
      
      <material name="Orange">
        <color rgba="${255/255} ${108/255} ${10/255} 1.0"/>
      </material>
    </visual>
    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
        <cylinder radius="0.05" length="0.025"/>
      </geometry>
    </collision>
  </link>


